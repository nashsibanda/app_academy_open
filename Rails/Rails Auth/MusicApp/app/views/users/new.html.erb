<h2>Sign Up!</h2>

<form action="/users" method="post" class="log_in_form">
  <%= auth_token %>

  <div class="log_in_form_row">
    <label for="sign_up_email">Email: </label>
    <input type="text" name="user[email]" id="sign_up_email">
  </div>

  <div class="log_in_form_row">
    <label for="sign_up_password">Password: </label>
    <input type="password" name="user[password]" id="sign_up_password">
  </div>
    
  <input type="submit" value="Sign Up!">
</form>